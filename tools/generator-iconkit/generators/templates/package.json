{
  "name": "<%= packageName %>",
  "version": "<%= versionNo %>",
  "description": "<%= familyName %> for @svgr-iconkit",
  "keywords": [
    "SVG",
    "Icons",
    "react",
    "react-native",
    "expo"
  ],
  "license": "<%= license %>",
  "main": "lib/commonjs/index",
  "react-native": "lib/es/native",
  "commonjs": "lib/commonjs/index",
  "module": "lib/es/index",
  "types": "lib/types/index",

  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.*.ts",
    "index.*.js",
    "native.js",
    "native.d.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0",
    "react": "^16.8.0 || ^17.0.1 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0"
  },
  "devDependencies": {
    "@svgr-iconkit/cli": "^0.3.0",
    "@svgr-iconkit/build-config": "^0.3.0",
    "@types/node": "^15.6.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:prepare": "rimraf ./src/gen",
    "iconsmap:create:<%= variantName %>": "svgr-iconkit convert-data -i assets/<%= variantName %> -o src/data/<%= variantName %>",
    "iconsmap:create": "npm-run-all iconsmap:create:<%= variantName %>",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all iconsmap compile"
  }
}
