{
  "name": "@svgr-iconkit/heroicons",
  "version": "0.3.0-beta.1",
  "description": "Heroicons for @svgr-iconkit",
  "keywords": [
    "SVG",
    "Icons",
    "react",
    "react-native",
    "expo"
  ],
  "license": "MIT",
  "repository": "https://github.com/svgr-iconkit/svgr-iconkit",
  "homepage": "https://svgr-iconkit.dev/explorer/heroicons",
  "originalAuthor": {
    "name": "Heroicons",
    "url": "https://npmjs.com/package/heroicons"
  },
  "main": "index",
  "react-native": "index",
  "commonjs": "index",
  "module": "index.esm",
  "types": "index",
  "directories": {
    "icons": "icons",
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.*.ts",
    "index.*.js",
    "native.js",
    "native.d.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.0",
    "react": "^16.8.0 || ^17.0.1 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-typescript": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@svgr-iconkit/build-config": "^0.3.0-beta.1",
    "@svgr-iconkit/cli": "^0.3.0-beta.1",
    "@types/node": "^15.6.1",
    "heroicons": "^1.0.6",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "terser": "^5.7.0",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:upgrade": "yarn add -D heroicons",
    "iconsmap:prepare": "rimraf ./src/gen",
    "iconsmap:create:solid": "svgr-iconkit convert-data -i solid -o src/gen/icons/solid --stroke-color 'currentColor' -p heroicons",
    "iconsmap:create:outline": "svgr-iconkit convert-data -i outline -o src/gen/icons/outline --stroke-color 'currentColor' -p heroicons",
    "iconsmap:create": "npm-run-all iconsmap:create:solid iconsmap:create:outline",
    "iconsmap": "npm-run-all iconsmap:prepare  iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all prebuild iconsmap compile"
  },
  "gitHead": "6277f374ee3386099173394f98c692819e4fc3cd"
}
