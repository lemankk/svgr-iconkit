{
  "name": "@svgr-iconkit/fluentui-system",
  "version": "0.3.0-beta.3",
  "description": "FluentUI System Icon for @svgr-iconkit",
  "keywords": [
    "SVG",
    "Icons",
    "react",
    "react-native",
    "expo"
  ],
  "repository": "https://github.com/svgr-iconkit/svgr-iconkit",
  "homepage": "https://svgr-iconkit.dev/explorer/fluentui-system",
  "originalAuthor": {
    "name": "@fluentui/svg-icons by Microsoft",
    "url": "https://github.com/microsoft/fluentui-system-icons.git"
  },
  "license": "MIT",
  "main": "index",
  "react-native": "index",
  "commonjs": "index",
  "module": "index.esm",
  "types": "index",
  "directories": {
    "icons": "icons",
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.*.ts",
    "index.*.js",
    "native.js",
    "native.d.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.0",
    "react": "^16.8.0 || ^17.0.1 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.3"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-typescript": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@fluentui/svg-icons": "^1.1.166",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@svgr-iconkit/build-config": "^0.3.0-beta.3",
    "@svgr-iconkit/cli": "^0.3.0-beta.3",
    "@types/node": "^15.6.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "terser": "^5.7.0",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:upgrade": "yarn add -D @fluentui/svg-icons",
    "iconsmap:prepare": "rimraf ./src/gen; mkdir -p ./src/gen/assets/regular; mkdir -p ./src/gen/assets/filled",
    "iconsmap:copy:regular": "cp -R ../../node_modules/@fluentui/svg-icons/icons/*_24_regular.svg ./src/gen/assets/regular",
    "iconsmap:copy:filled": "cp -R ../../node_modules/@fluentui/svg-icons/icons/*_24_*.svg ./src/gen/assets",
    "iconsmap:create:regular": "svgr-iconkit convert-data -i icons -o src/data/regular --end-with '_24_regular' --remove-name-suffix '_24_regular' -p @fluentui/svg-icons",
    "iconsmap:create:filled": "svgr-iconkit convert-data -i icons -o src/data/filled --end-with '_24_filled' --remove-name-suffix '_24_filled' -p @fluentui/svg-icons",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:create:regular iconsmap:create:filled",
    "prebuild": "rimraf ./lib ./dist ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all prebuild iconsmap compile"
  },
  "gitHead": "6277f374ee3386099173394f98c692819e4fc3cd"
}
