{
  "name": "@svgr-iconkit/material-community",
  "version": "0.3.0-beta.0",
  "description": "Material Community Icon for @svgr-iconkit",
  "keywords": [
    "SVG",
    "Icons",
    "react",
    "react-native",
    "expo"
  ],
  "license": "Apache-2.0",
  "repository": "https://github.com/svgr-iconkit/svgr-iconkit",
  "homepage": "https://svgr-iconkit.dev/explorer/material-community",
  "originalAuthor": {
    "name": "Material Design Icons by community",
    "url": "https://materialdesignicons.com/"
  },
  "main": "index",
  "react-native": "index",
  "commonjs": "index",
  "module": "index.esm",
  "types": "index",
  "directories": {
    "icons": "icons",
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.*.ts",
    "index.*.js",
    "native.js",
    "native.d.ts",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.0",
    "react": "^16.8.0 || ^17.0.1"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-typescript": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@mdi/svg": "^6.6.96",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@svgr-iconkit/build-config": "^0.3.0-beta.0",
    "@svgr-iconkit/cli": "^0.3.0-beta.0",
    "@svgr-iconkit/rollup-plugin-svg-data": "^0.3.0-beta.0",
    "@types/node": "^15.6.1",
    
    "@types/react-native": "^0.64.10",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "terser": "^5.7.0",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:prepare": "rimraf ./src/gen; mkdir -p ./src/gen/svgs; mkdir -p ./src/gen/assets/outline; mkdir -p ./src/gen/assets/regular",
    "iconsmap:copy:outline": "svgr-iconkit deep-copy -i svg -p @mdi/svg -o src/gen/assets/outline -E '-outline.svg' --remove-name-suffix '-outline'",
    "iconsmap:copy:regular": "svgr-iconkit deep-copy -i svg -p @mdi/svg -o src/gen/assets/regular --not-ends-with '-outline.svg'",
    "iconsmap:copy": "npm-run-all iconsmap:copy:regular iconsmap:copy:outline",
    "iconsmap:create:outline": "svgr-iconkit convert-data -i src/gen/assets/outline -o src/gen/icons/outline",
    "iconsmap:create:regular": "svgr-iconkit convert-data -i src/gen/assets/regular -o src/gen/icons/regular",
    "iconsmap:create": "npm-run-all iconsmap:create:regular iconsmap:create:outline",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:copy iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./tmp ./gen ./icons",
    "compile": "NODE_OPTIONS=--max-old-space-size=8192 rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all prebuild iconsmap compile"
  },
  "gitHead": "6277f374ee3386099173394f98c692819e4fc3cd"
}
