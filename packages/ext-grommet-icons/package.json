{
  "name": "@svgr-iconkit/grommet-icons",
  "version": "0.3.0-beta.13",
  "description": "Grommet Icons for @svgr-iconkit",
  "keywords": [
    "SVG",
    "Icons",
    "react",
    "react-native",
    "expo"
  ],
  "license": "MIT",
  "repository": "https://github.com/svgr-iconkit/svgr-iconkit",
  "homepage": "https://svgr-iconkit.dev/explorer/grommet-icons",
  "originalAuthor": {
    "name": "grommet-icons",
    "url": "https://github.com/grommet/grommet-icons"
  },
  "main": "lib/commonjs/index",
  "react-native": "lib/es/native",
  "commonjs": "lib/commonjs/index",
  "module": "lib/es/index",
  "types": "lib/types/index",
  "directories": {
    "icons": "icons",
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.*.ts",
    "index.*.js",
    "native.js",
    "native.d.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.0",
    "react": "^16.8.0 || ^17.0.1 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.13"
  },
  "devDependencies": {
    "@svgr-iconkit/build-config": "^0.3.0-beta.11",
    "@svgr-iconkit/cli": "^0.3.0-beta.10",
    "@types/node": "^15.6.1",
    "grommet-icons": "^4.7.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:prepare": "rimraf ./src/gen",
    "iconsmap:create:regular": "svgr-iconkit convert-data -i img -p grommet-icons -o src/data/regular",
    "iconsmap:create": "npm-run-all iconsmap:create:regular",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all iconsmap compile"
  }
}
