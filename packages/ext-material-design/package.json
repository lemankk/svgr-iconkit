{
  "name": "@svgr-iconkit/material-design",
  "version": "0.1.13-alpha.2",
  "description": "Material Design Icons SVGR iconset",
  "keywords": [
    "SVG",
    "Icons"
  ],
  "license": "MIT",
  "main": "index",
  "react-native": "index.native.js",
  "commonjs": "lib/commonjs/index.js",
  "module": "lib/es/index.js",
  "types": "index",
  "directories": {
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "lib",
    "native",
    "index.js",
    "index.ios.js",
    "index.android.js",
    "index.native.js",
    "index.native.d.ts",
    "index.d.ts",
    "native.js",
    "native.d.ts",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.1.1",
    "react": "^16.8.0 || ^17.0.1",
    "react-native-svg": "^12.1.0"
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.1.13-alpha.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-typescript": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@rollup/plugin-typescript": "^8.2.1",
    "@svgr-iconkit/build-utils": "^0.1.13-alpha.0",
    "@svgr-iconkit/cli": "^0.1.13-alpha.0",
    "@svgr-iconkit/rollup-plugin-svg-data": "^0.1.13-alpha.0",
    "@types/node": "^15.6.1",
    "@types/react": "^17.0.9",
    "@types/react-native": "^0.64.10",
    "npm-run-all": "^4.1.5",
    "react": "16.13.1",
    "react-native": "^0.64.2",
    "react-native-builder-bob": "^0.18.1",
    "react-native-svg": "12.1.0",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.4",
    "rollup-plugin-dts": "^3.0.1",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "terser": "^5.7.0",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:prepare": "mkdir -p ./src/gen/assets",
    "iconsmap:copy": "svgr-iconkit deep-copy -C svg/production -E _24px.svg ../../node_modules/material-design-icons/ ./src/gen/assets",
    "iconsmap:create:regular": "svgr-iconkit build-map ./src/gen/assets ./src/gen/icons/regular.ts --remove-name-prefix='ic_' --remove-name-suffix='_24px'",
    "iconsmap:create": "npm-run-all iconsmap:create:regular",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:copy iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./tmp ./src/gen ./gen",
    "compile": "rollup -c",
    "build": "npm-run-all iconsmap compile"
  }
}
