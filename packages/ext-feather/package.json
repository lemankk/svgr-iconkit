  "scripts": {
    "iconsmap:create-folder": "mkdir -p ./tmp/icons",
    "iconsmap:copy-source": "cp -R ../../node_modules/feather-icons/dist/icons/*.svg ./tmp/icons",
    "iconsmap:create": "svgr-iconkit build-map ./tmp/icons ./src/icons.ts",
    "iconsmap": "npm-run-all iconsmap:create-folder iconsmap:copy-source iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./tmp",
    "compile": "rollup -c",
    "build": "npm-run-all iconsmap compile"
  },
