    "native.d.ts",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.2.0",
    "react": "^16.8.0 || ^17.0.1"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.2.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.14.3",
    "@babel/core": "^7.14.3",
    "@babel/plugin-transform-typescript": "^7.14.4",
    "@babel/preset-env": "^7.14.4",
    "@babel/preset-react": "^7.13.13",
    "@rollup/plugin-babel": "^5.3.0",
    "@rollup/plugin-commonjs": "^19.0.0",
    "@rollup/plugin-json": "^4.1.0",
    "@rollup/plugin-node-resolve": "^13.0.0",
    "@svgr-iconkit/build-config": "^0.2.0",
    "@svgr-iconkit/cli": "^0.2.0",
    "@svgr-iconkit/rollup-plugin-svg-data": "^0.2.0",
    "@types/node": "^15.6.1",
    "@types/react-native": "^0.64.10",
    "fontisto": "git+https://github.com/kenangundogan/fontisto",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.4",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.30.0",
    "terser": "^5.7.0",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:prepare": "rimraf ./src/gen; mkdir -p ./src/gen/assets/regular; mkdir -p ./src/gen/assets/sharp; mkdir -p ./src/gen/assets/outline",
    "iconsmap:copy": "svgr-iconkit deep-copy -E .svg -i icons -p fontisto -o src/gen/assets",
    "iconsmap:create:regular": "svgr-iconkit convert-data -i src/gen/assets -o src/gen/icons/regular",
    "iconsmap:create": "npm-run-all iconsmap:create:regular",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:copy iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./tmp ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all iconsmap compile"
  },
  "gitHead": "6277f374ee3386099173394f98c692819e4fc3cd"
}
