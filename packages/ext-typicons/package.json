{
  "name": "@svgr-iconkit/typicons",
  "version": "0.3.0",
  "description": "Typicons for @svgr-iconkit",
  "keywords": [
    "SVG",
    "Icons",
    "react",
    "react-native",
    "expo"
  ],
  "license": "CC-BY-SA",
  "repository": "https://github.com/svgr-iconkit/svgr-iconkit",
  "homepage": "https://svgr-iconkit.dev/explorer/typicons",
  "originalAuthor": {
    "name": "Typicons",
    "url": "https://www.s-ings.com/typicons"
  },
  "exports": {
    "./icons/*": "./icons/*.js",
    "./native": {
      "require": "./lib/cjs/native.js",
      "browser": "./lib/cjs/native.js",
      "import": "./lib/esm/native.js",
      "types": "./lib/types/native",
      "default": "./lib/esm/native.js"
    },
    ".": {
      "react-native": "./lib/esm/native.js",
      "import": "./lib/esm/index.js",
      "default": "./lib/cjs/index.js"
    }
  },
  "main": "lib/cjs/index",
  "react-native": "lib/esm/native",
  "commonjs": "lib/cjs/index",
  "module": "lib/esm/index",
  "types": "lib/types/index",
  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.*.ts",
    "index.*.js",
    "native.js",
    "native.d.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.14",
    "react": "^16.8.0 || ^17.0.1 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0"
  },
  "devDependencies": {
    "@svgr-iconkit/build-config": "^0.3.0",
    "@svgr-iconkit/cli": "^0.3.0",
    "@types/node": "^15.6.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "rollup": "^2.50.4",
    "typescript": "^4.3.4",
    "typicons.font": "^2.1.2"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:upgrade": "yarn add -D typicons.font",
    "iconsmap:prepare": "rimraf ./src/gen",
    "iconsmap:copy:regular": "svgr-iconkit deep-copy -i src/svg -o ./src/gen/assets/regular/ -p typicons.font --not-ends-with '-outline.svg'",
    "iconsmap:copy:outline": "svgr-iconkit deep-copy -i src/svg -o ./src/gen/assets/outline/ -p typicons.font --ends-with '-outline.svg' --remove-name-suffix '-outline'",
    "iconsmap:copy": "npm-run-all iconsmap:copy:regular iconsmap:copy:outline",
    "iconsmap:create:regular": "svgr-iconkit convert-data -i ./src/gen/assets/regular/ -o ./src/data/regular/",
    "iconsmap:create:outline": "svgr-iconkit convert-data -i ./src/gen/assets/outline/ -o ./src/data/outline/",
    "iconsmap:create": "npm-run-all iconsmap:create:regular iconsmap:create:outline",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:copy iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all prebuild iconsmap compile"
  },
  "gitHead": "6277f374ee3386099173394f98c692819e4fc3cd"
}
