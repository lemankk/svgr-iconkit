{
  "name": "@svgr-iconkit/flat-color-icons",
  "version": "0.3.0-beta.5",
  "description": "flat-color-icons for @svgr-iconkit",
  "keywords": [
    "SVG",
    "Icons",
    "react",
    "react-native",
    "expo"
  ],
  "license": "Good Boy License",
  "repository": "https://github.com/svgr-iconkit/svgr-iconkit",
  "homepage": "https://svgr-iconkit.dev/explorer/flat-color-icons",
  "originalAuthor": {
    "name": "flat-color-icons",
    "url": "https://github.com/icons8/flat-color-icons"
  },
  "main": "lib/commonjs/index",
  "react-native": "lib/commonjs/native",
  "commonjs": "lib/commonjs/index",
  "module": "lib/es/index",
  "types": "index",
  "directories": {
    "icons": "icons",
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.*.ts",
    "index.*.js",
    "native.js",
    "native.d.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.0",
    "react": "^16.8.0 || ^17.0.1 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.5"
  },
  "devDependencies": {
    "@svgr-iconkit/build-config": "^0.3.0-beta.5",
    "@svgr-iconkit/cli": "^0.3.0-beta.5",
    "@types/node": "^15.6.1",
    "flat-color-icons": "^1.1.0",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:upgrade": "yarn add -D flat-color-icons",
    "iconsmap:prepare": "rimraf ./src/gen",
    "iconsmap:copy:regular": "svgr-iconkit deep-copy -i svg -o ./src/gen/assets/regular/ -p flat-color-icons",
    "iconsmap:copy": "npm-run-all iconsmap:copy:regular",
    "iconsmap:create:regular": "svgr-iconkit convert-data -i ./src/gen/assets/regular/ -o ./src/data/regular/",
    "iconsmap:create": "npm-run-all iconsmap:create:regular",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:copy iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all prebuild iconsmap compile"
  },
  "gitHead": "6277f374ee3386099173394f98c692819e4fc3cd"
}
