  ],
  "license": "MIT",
  "repository": "https://github.com/svgr-iconkit/svgr-iconkit",
  "homepage": "https://svgr-iconkit.dev/explorer/flag-icons",
  "originalAuthor": {
    "name": "flag-icons",
    "url": "https://flagicons.lipis.dev/"
  },
  "main": "lib/commonjs/index",
  "react-native": "lib/es/native",
  "commonjs": "lib/commonjs/index",
  "module": "lib/es/index",
  "types": "lib/types/index",
  "directories": {
    "icons": "icons",
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.ios.js",
    "index.android.js",
    "index.native.js",
    "index.native.d.ts",
    "index.web.js",
    "index.web.d.ts",
    "index.d.ts",
    "native.js",
    "native.d.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.0",
    "react": "^16.8.0 || ^17.0.1 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-native": {
      "optional": true
    },
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.10"
  },
  "devDependencies": {
    "@svgr-iconkit/build-config": "^0.3.0-beta.10",
    "@svgr-iconkit/cli": "^0.3.0-beta.10",
    "@types/node": "^15.6.1",
    "flag-icons": "^6.1.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:upgrade": "yarn add -D flag-icons",
    "iconsmap:prepare": "rimraf ./src/gen",
    "iconsmap:create:square": "svgr-iconkit convert-data -i flags/1x1 -o src/data/square -p flag-icons",
    "iconsmap:create:regular": "svgr-iconkit convert-data -i flags/4x3 -o src/data/regular -p flag-icons",
    "iconsmap:create": "npm-run-all iconsmap:create:square iconsmap:create:regular",
    "iconsmap": "npm-run-all iconsmap:prepare iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all prebuild iconsmap compile"
  },
  "gitHead": "6277f374ee3386099173394f98c692819e4fc3cd"
}
