{
  "name": "@svgr-iconkit/fontawesome5",
  "version": "0.3.0-beta.13",
  "description": "FontAwesome (regular) for @svgr-iconkit",
  "keywords": [
    "SVG",
    "Icons",
    "react",
    "react-native",
    "expo"
  ],
  "repository": "https://github.com/svgr-iconkit/svgr-iconkit",
  "homepage": "https://svgr-iconkit.dev/explorer/fontawesome5",
  "originalAuthor": {
    "name": "Font Awesome",
    "url": "https://www.npmjs.com/package/@fortawesome/fontawesome-free"
  },
  "license": "MIT",
  "main": "lib/commonjs/index",
  "react-native": "lib/es/native",
  "commonjs": "lib/commonjs/index",
  "module": "lib/es/index",
  "types": "lib/types/index",
  "directories": {
    "icons": "icons",
    "lib": "lib",
    "native": "native"
  },
  "files": [
    "icons",
    "lib",
    "native",
    "index.js",
    "index.*.ts",
    "index.*.js",
    "native.js",
    "native.d.ts",
    "LICENSE.md",
    "README.md"
  ],
  "peerDependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.0",
    "react": "^16.8.0 || ^17.0.1 || ^18.0.0"
  },
  "peerDependenciesMeta": {
    "react-native-svg": {
      "optional": true
    }
  },
  "dependencies": {
    "@svgr-iconkit/core": "^0.3.0-beta.13"
  },
  "devDependencies": {
    "@fortawesome/fontawesome-free": "^5.0.0",
    "@svgr-iconkit/build-config": "^0.3.0-beta.11",
    "@svgr-iconkit/cli": "^0.3.0-beta.10",
    "@types/node": "^15.6.1",
    "npm-run-all": "^4.1.5",
    "react": "^17.0.1",
    "rimraf": "^3.0.2",
    "typescript": "^4.3.4"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "iconsmap:upgrade": "yarn add -D @fortawesome/fontawesome-free",
    "iconsmap:prepare": "rimraf ./src/gen",
    "iconsmap:create:regular": "svgr-iconkit convert-data -i svgs/regular -o src/data/regular -p @fortawesome/fontawesome-free",
    "iconsmap:create:solid": "svgr-iconkit convert-data -i svgs/solid -o ./src/data/solid -p @fortawesome/fontawesome-free",
    "iconsmap:create": "npm-run-all iconsmap:create:regular iconsmap:create:solid",
    "iconsmap": "npm-run-all iconsmap:create",
    "prebuild": "rimraf ./lib ./dist ./gen ./icons",
    "compile": "rollup -c",
    "build": "npm-run-all compile",
    "build:full": "npm-run-all prebuild iconsmap compile"
  },
  "gitHead": "6277f374ee3386099173394f98c692819e4fc3cd"
}
